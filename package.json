{
  "name": "salat-time",
  "description": "Location based daily salat schedule",
  "main": "index.js",
  "scripts": {
    "start": "npm run clean && npm run webpack-dev",
    "build": "npm run clean && npm run webpack-build",
    "clean": "node task/clean",
    "webpack-build": "NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack.prod.config.js",
    "webpack-dev": "NODE_ENV=development webpack-dev-server --https --inline --hot --colors --config ./config/webpack.dev.config.js",
    "test": "karma start config/karma.conf.js",
    "lint": "eslint src/*/**/*.js --fix",
    "format": "prettier src/*/**/*.js --write"
  },
  "author": "Danu Widatama",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/widatama/salat-time.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-0"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.0.1",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.1.0",
    "karma-tap": "^4.1.3",
    "karma-tap-pretty-reporter": "^4.1.0",
    "karma-webpack": "^3.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "style-loader": "^0.23.1",
    "tap-summary": "^4.0.0",
    "tape": "^4.10.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.9",
    "webapp-webpack-plugin": "^2.6.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-stylish": "^0.1.8",
    "workbox-webpack-plugin": "^4.1.1"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.8",
    "jstz": "^2.1.1",
    "normalize.css": "^8.0.1",
    "url-parse": "^1.4.4",
    "vue": "^2.6.9",
    "vuex": "^3.1.0"
  }
}
